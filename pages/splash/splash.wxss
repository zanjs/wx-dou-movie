.splash {
  flex: 1;
}
.splash swiper-item {
  flex: 1;
}
.splash swiper-item image {
  position: absolute;
  height: 100%;
  width: 100%;
  opacity: .6;
}
.start {
  position: absolute;
  bottom: 200rpx;
  left: 50%;
  width: 300rpx;
  margin-left: -150rpx;
  background-color: rgba(53, 73, 94, 0.8);
  color: #fff;
  border: 5rpx solid #35495e;
  font-size: 40rpx;
}
